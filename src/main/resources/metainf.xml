<?xml version="1.0" encoding="UTF-8"?>
<SetIntegration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" set10-api-version="0.0.3" version="1.0.0"
                xmlns="http://crystals.ru/set10/api/metainf"
                xsi:schemaLocation="http://crystals.ru/set10/api/metainf metainf.xsd ">
    <ExternalService serviceType="PAYMENT" localeKey="kaspi.qr.service.name" id="kaspi.qr.payment.service">
        <Description>Kaspi QR</Description>
        <Options>
            <String key="config.file.path.windows" localeKey="path.windows.description"/>
            <String key="config.file.path.linux" localeKey="path.linux.description"/>
        </Options>
        <PaymentPlugin paymentType="ELECTRONIC" localeKey="kaspi.qr.plugin.name" id="kaspi.qr.payment.plugin">
            <Description>Kaspi QR</Description>
            <PersistedField key="kaspi.qr.payment.id" localeKey="kaspi.qr.payment.id.field.name" exportable="true"
                            visible="true"/>
            <PersistedField key="kaspi.qr.payment.amount" localeKey="kaspi.qr.payment.amount.field.name" exportable="true"
                            visible="true"/>
            <PersistedField key="kaspi.qr.terminal.number" localeKey="kaspi.qr.terminal.no.field.name" exportable="true"
                            visible="true"/>
            <PersistedField key="kaspi.qr.transaction.date" localeKey="kaspi.qr.transaction.date.field.name"
                            exportable="true"
                            visible="true"/>
            <PersistedField key="kaspi.qr.bank.id" localeKey="kaspi.qr.bank.id.field.name" exportable="true"
                            visible="true"/>
            <PersistedField key="kaspi.qr.payment.method" localeKey="kaspi.qr.payment.method.field.name"
                            exportable="true"
                            visible="true"/>
            <PersistedField key="kaspi.qr.payment.address" localeKey="kaspi.qr.payment.address.field.name"
                            exportable="true" visible="true"/>
            <PersistedField key="kaspi.qr.payment.city" localeKey="kaspi.qr.payment.city.field.name" exportable="true"
                            visible="true"/>
            <PersistedField key="kaspi.qr.payment.store.name" localeKey="kaspi.qr.payment.store.name.field.name"
                            exportable="true" visible="true"/>
            <PersistedField key="kaspi.qr.payment.loan.offer.name" localeKey="kaspi.qr.payment.loan.term.field.name"
                            exportable="true" visible="true"/>
            <PersistedField key="kaspi.qr.payment.loan.term" localeKey="kaspi.qr.payment.loan.offer.name"
                            exportable="true" visible="true"/>
            <PersistedField key="kaspi.qr.payment.is.offer" localeKey="kaspi.qr.payment.is.offer.field.name"
                            exportable="true" visible="true"/>
        </PaymentPlugin>
    </ExternalService>
</SetIntegration>
